{
	"core": {
		"environment": "dev"
	},
	"logger": {
		"level": "info",
		"color": true,
		"file": null,
		"dirname": ".",
		"to_json": false,
		"rotation": ".yyyy-MM-Tdd"
	},
	"www-server": {
		"port": 8080,
		"api-base": "/api/v1",
		"session-cookie-name": "sealious-session",
		"anonymous-cookie-name": "sealious-anon",
		"max-payload-bytes": 10485760
	},
	"datastore_mongo": {
		"embedded": false,
		"host": "localhost",
		"port": 27017,
		"db_name": "sealious"
	},
	"roles": ["admin"],
	"password_hash": {
		"iterations": 26000,
		"key_length": 512,
		"salt_length": 21
	}
}
